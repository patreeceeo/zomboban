{
  "name": "zomboban",
  "packageManager": "npm@10.8.2",
  "version": "0.0.0",
  "type": "module",
  "files": [],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev-api": "NODE_ENV=development SERVER_PORT=3001 tsx watch.ts ./src/server/index.ts",
    "dev-vite": "NODE_ENV=development vite dev --port 3000",
    "dev": "conc npm:dev-vite npm:dev-api",
    "serve": "tsx ./src/server/index.ts",
    "signUp": "tsx ./signUp.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "NODE_ENV=test tsx test.ts",
    "test-dev": "NODE_OPTIONS=\"--max_old_space_size=8192 --trace-warnings\" NODE_ENV=test nodemon test.ts -w \"**/*.ts\"",
    "type-check": "tsc --noEmit",
    "graph-deps": "madge --image dependency_graph_client.svg src/client.ts && madge --image dependency_graph_server.svg src/server/index.ts && madge --image dependency_graph_zui.svg src/Zui/index.ts",
    "bench": "tsx src/benchmark.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@marko/compiler": "^5.39.33",
    "@marko/vite": "^5.3.0",
    "@playwright/test": "^1.45.1",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/html-vite": "^9.0.16",
    "@tauri-apps/cli": "^1.5.6",
    "@types/chokidar": "^2.1.3",
    "@types/compression": "^1",
    "@types/cookie-parser": "^1",
    "@types/express": "^4.17.18",
    "@types/express-session": "^1",
    "@types/lodash": "^4.14.200",
    "@types/madge": "^5.0.3",
    "@types/node": "^20.8.7",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1",
    "@types/path-to-regexp": "^1.7.0",
    "@types/session-file-store": "^1",
    "@types/three": "^0.178.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "glob": "^10.3.10",
    "madge": "^6.1.0",
    "marko": "^5.37.49",
    "nodemon": "^3.0.1",
    "picocolors": "^1.0.0",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "storybook": "^9.0.16",
    "tsc-files": "^1.1.4",
    "tslib": "^2.6.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8",
    "vite": "^5.3.5"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-serve-static-core": "^0.1.1",
    "express-session": "^1.18.0",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^8.2.0",
    "session-file-store": "^1.5.0",
    "three": "^0.178.0"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "tsx"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "bracketSpacing": true
  }
}
